<yandex>
    <!-- Users section -->
    <users>
        <default>
            <password></password>
            <networks>
                <ip>::/0</ip> <!-- Allow access from anywhere -->
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>

        <!-- Root user definition -->
        <root>
            <password></password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>root</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </root>
    </users>

    <!-- Profiles section, default and root profiles for users -->
    <profiles>
        <default>
            <!-- Performance parameters for default profile -->
            <max_threads>8</max_threads>
            <async_insert_threads>8</async_insert_threads>
            <max_block_size>655360000</max_block_size>
            <max_insert_block_size>655360000</max_insert_block_size>
            <max_memory_usage>150000000000</max_memory_usage>
            <max_network_bandwidth>10000000000</max_network_bandwidth>
            <max_query_size>1048576000</max_query_size>
            <max_rows_to_read>1000000000</max_rows_to_read>
            <max_memory_usage_for_all_queries>200000000000</max_memory_usage_for_all_queries>
            <use_uncompressed_cache>1</use_uncompressed_cache>
            <load_balancing>random</load_balancing>
            <background_pool_size>32</background_pool_size>
            <async_insert>1</async_insert>
            <wait_for_async_insert>0</wait_for_async_insert>
            <async_insert_busy_timeout_max_ms>1000000</async_insert_busy_timeout_max_ms>
            <async_insert_busy_timeout_min_ms>100000</async_insert_busy_timeout_min_ms>
            <async_socket_for_remote>1</async_socket_for_remote>
            <async_insert_max_data_size>1048576000</async_insert_max_data_size>
            <async_insert_max_query_number>1000000</async_insert_max_query_number> 
            <async_query_sending_for_remote>1</async_query_sending_for_remote>
            <allow_unrestricted_reads_from_keeper>true</allow_unrestricted_reads_from_keeper>
            <max_insert_threads>8</max_insert_threads>
        </default>

        <root>
            <!-- Performance parameters for root profile -->
            <max_threads>8</max_threads>
            <async_insert_threads>8</async_insert_threads>
            <max_block_size>655360000</max_block_size>
            <max_insert_block_size>655360000</max_insert_block_size>
            <max_memory_usage>150000000000</max_memory_usage>
            <max_network_bandwidth>10000000000</max_network_bandwidth>
            <max_query_size>1048576000</max_query_size>
            <max_rows_to_read>1000000000</max_rows_to_read>
            <max_memory_usage_for_all_queries>200000000000</max_memory_usage_for_all_queries>
            <use_uncompressed_cache>1</use_uncompressed_cache>
            <load_balancing>random</load_balancing>
            <background_pool_size>32</background_pool_size>
            <async_insert>1</async_insert>
            <wait_for_async_insert>0</wait_for_async_insert>
            <async_insert_busy_timeout_max_ms>10000000</async_insert_busy_timeout_max_ms>
            <async_insert_busy_timeout_min_ms>100000</async_insert_busy_timeout_min_ms>
            <async_socket_for_remote>1</async_socket_for_remote>
            <async_insert_max_data_size>1048576000</async_insert_max_data_size>
            <async_insert_max_query_number>1000000</async_insert_max_query_number> 
            <async_query_sending_for_remote>1</async_query_sending_for_remote>
            <allow_unrestricted_reads_from_keeper>true</allow_unrestricted_reads_from_keeper>
            <max_insert_threads>8</max_insert_threads>
        </root>
    </profiles>

    <!-- Quotas section, default quotas for users -->
    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</yandex>

