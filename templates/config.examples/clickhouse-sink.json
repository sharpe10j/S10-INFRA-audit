{
  "name": "clickhouse-sink",
  "config": {
    "connector.class": "com.clickhouse.kafka.connect.ClickHouseSinkConnector",
    "tasks.max": "4",
    "topics": "docker_topic_1",
    "hostname": "10.0.0.208",
    "port": "8123",
    "clickhouse.user": "default",
    "clickhouse.password": "default",
    "hostname": "10.0.0.208",
    "database": "database1",
    "topic2TableMap": "docker_topic_1=production_test_table_1",
    "auto.create.table": "false",
    "batch.size": "50000",
    "linger.ms": "500",
    "consumer.override.fetch.min.bytes": "52428800",
    "consumer.override.fetch.max.bytes": "524288000",
    "consumer.override.fetch.max.wait.ms": "500",
    "consumer.override.max.partition.fetch.bytes": "52428800",
    "consumer.override.max.poll.records": "100000",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": false
  }
}
